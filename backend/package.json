{
  "name": "ai-chat-backend",
  "version": "1.0.0",
  "description": "Backend API for AI Chat Interface",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "init-db": "node scripts/initDatabase.js",
    "build": "tsc",
    "type-check": "tsc --noEmit",
    "dev:ts": "nodemon --exec \"npm run type-check && node\" server.js"
  },
  "dependencies": {
    "axios": "^1.6.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "joi": "^17.13.3",
    "sqlite3": "^5.1.6",
    "uuid": "^9.0.1",
    "validator": "^13.15.15",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/node": "^24.2.1",
    "@types/validator": "^13.15.2",
    "nodemon": "^3.0.2",
    "typescript": "^5.9.2"
  },
  "keywords": [
    "ai",
    "chat",
    "api",
    "express",
    "sqlite"
